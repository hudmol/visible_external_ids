<section id="external_ids" class="subrecord-form-dummy">
  <h3><%= I18n.t("external_id._plural")%></h3>
  <div class="subrecord-form-container">
    <% ASUtils.wrap(external_ids).each do |external_id| %>
      <div class="subrecord-form-fields">
        <div class="form-group">
          <label class="col-sm-2 control-label">
            <%= I18n.t("external_id.source") %>
          </label>
          <div class="col-sm-9">
            <div class="controls label-only">
              <%= external_id['source'] %>
            </div>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label">
            <%= I18n.t("external_id.external_id") %>
          </label>
          <div class="col-sm-9">
            <div class="controls label-only">
              <%= external_id['external_id'] %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</section>
