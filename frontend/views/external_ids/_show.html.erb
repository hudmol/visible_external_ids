<section id="visible_external_ids" class="subrecord-form-dummy">
  <h3><%= I18n.t("plugins.visible_external_ids._plural")%></h3>
  <div class="subrecord-form-container">
    <div class="subrecord-form-fields">
      <table class="table table-striped table-bordered table-condensed table-hover token-list">
        <thead>
        <tr>
          <td><%= I18n.t("plugins.visible_external_ids.source") %></td>
          <td><%= I18n.t("plugins.visible_external_ids.external_id") %></td>
        </tr>
        </thead>
        <tbody>
          <% ASUtils.wrap(external_ids).each do |external_id| %>
            <tr>
              <td><%= external_id['source'] %></td>
              <td><%= external_id['external_id'] %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</section>